{
  "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "rootDir": ".",
      "module": "CommonJS",
      "target": "ES6",
      "outDir": "dist",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true
  },
  
}